    "paths": {
      "/ResBrevetBac": {
        "get": {
          "tags": ["Download the data"],
          "summary": "Get the result of bac and brevet",
          "responses": {
            "200": {
              "description": "Transform csv file to a JSon file and return it",
              "schema": {
                "$ref": "#/definitions/CSV"
              }
            }
          }
        }
      },
      "/dlInternet": {
        "get": {
          "tags": ["Download the data"],
          "summary": "Get the result of connexion internet",
          "responses": {
            "200": {
              "description": "Get the data from an api and return a JSon file",
              "schema": {
                "$ref": "#/definitions/API"
              }
            }
          }
        }
    },

    "/dlBio": {
        "get": {
          "tags": ["Download the data"],
          "summary": "Get the quality of biology",
          "responses": {
            "200": {
              "description": "Create a JSon file after scrapping a Wikipedia page",
              "schema": {
                "$ref": "#/definitions/Scrap"
              }
            }
          }
        }
      },
    "/Resgaz": {
        "get": {
          "tags": ["Download the data"],
          "summary": "Get the quality of gaz",
          "responses": {
            "200": {
              "description": "Extract csv file to a JSon file and return it",
              "schema": {
                "$ref": "#/definitions/Scrap"
              }
            }
          }
        }
      }


    },


      
