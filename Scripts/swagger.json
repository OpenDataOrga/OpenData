              "uniqueItems": true
            },
            "Commune": {
              "type": "string"
            },
            "Brevet": {
              "type": "number"
            },
            "Bac": {
              "type": "number"
            },
            "internet": {
              "type": "number"
            },
            "bio": {
              "type": "number"
            },
            "gaz": {
              "type": "number"
            } 

          }
        }
        



      }
        
      
    
